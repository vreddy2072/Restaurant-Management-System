{
  "version": 2,
  "installCommand": "cd frontend && npm install --legacy-peer-deps",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://restaurant-ordering-system-5c3x.onrender.com/api/:path*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
